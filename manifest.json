{
  "manifest_version": 2,
  "name": "MouseX",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": ["script.js", "ufo.gif"]
}
